{
  "name": "instalura",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore .",
    "custom-script": "node ./script-test.js",
    "prepare": "husky install",
    "commit": "cz",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.jsx\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration:open": "cypress open",
    "test:integration": "cypress run --headless",
    "test:integration:build": "start-server-and-test build/start http-get://localhost:3000 test:integration",
    "build/start": "npm run build && npm start"
  },
  "dependencies": {
    "@crello/react-lottie": "0.0.11",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "jest-canvas-mock": "^2.3.1",
    "jest-styled-components": "^7.0.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "next": "latest",
    "nookies": "^2.5.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "styled-components": "^5.3.0",
    "styled-icons": "^10.34.0",
    "styled-normalize": "^8.0.7",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "babel-plugin-styled-components": "^1.12.0",
    "cypress": "^7.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "shelljs": "^0.8.4",
    "start-server-and-test": "^1.12.5"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
